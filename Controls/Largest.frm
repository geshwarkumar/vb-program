Private Sub btnCalculate_Click()
Dim num1, num2, num3 As Integer
num1 = Val(txtnum1.Text)
num2 = Val(txtnum2.Text)
num3 = Val(txtnum3.Text)
If num1 > num2 And num1 > num3 Then
    txtResult = num1
ElseIf num2 > num3 Then
    txtResult = num2
Else
    txtResult = num3
End If
End Sub

